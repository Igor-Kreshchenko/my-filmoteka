{{#each this}}
<li class="movies__list-item">
    <img class="movie__poster" src="https://image.tmdb.org/t/p/w300{{poster_path}}" alt="{{title}} poster" id="{{id}}"
        loading="lazy" width="300" height="450">

    <h2 class="movie__name">{{original_title}}</h2>
    <p class="movie__genre">{{genre_ids}} | {{release_date}}</p>
</li>
{{/each}}